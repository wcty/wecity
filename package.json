  "scripts": {
    "start": "concurrently \"react-scripts start\" \"npm run generate-watch\"",
    "prebuild": "npm run i18n && npm run generate",
    "build-zip": "react-scripts build && gzipper c -e br --verbose ./build && gzipper c --brotli -e gz --verbose ./build",
    "build": "react-scripts build",
    "i18n": "node scripts/i18n.js",
    "generate": "dotenv -- graphql-codegen --config scripts\\codegen.yml",
    "generate-watch": "dotenv -- graphql-codegen --config scripts\\codegen.yml --watch",
    "postbuild": "react-snap",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
